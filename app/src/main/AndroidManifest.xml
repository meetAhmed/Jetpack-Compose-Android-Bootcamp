            android:exported="false"
            android:label="@string/title_activity_constraint_layout"
            android:theme="@style/Theme.JetpackComposeBootcamp" />
        <activity
            android:name=".ComposeList"
            android:exported="false"
            android:label="@string/title_activity_compose_list"
            android:theme="@style/Theme.JetpackComposeBootcamp" />
        <activity
            android:name=".TextFieldAndButton"
            android:exported="false"
            android:label="@string/title_activity_text_field_and_button"
            android:theme="@style/Theme.JetpackComposeBootcamp" />
        <activity
