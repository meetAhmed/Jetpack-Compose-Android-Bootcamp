        <activity
            android:name=".Modifiers"
            android:exported="false"
            android:label="@string/title_activity_modifiers"
            android:theme="@style/Theme.JetpackComposeBootcamp" />
        <activity
            android:name=".RowAndColumn"
            android:exported="false"
            android:label="@string/title_activity_row_and_column"
            android:theme="@style/Theme.JetpackComposeBootcamp" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetpackComposeBootcamp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>